<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jameshachtel.concentration" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Classic Concentration</name>
    <description>Classic Concentration for mobile</description>
    <author email="jhachtel54@gmail.com" href="https://cordova.apache.org">
        James Hachtel
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="Fullscreen" value="true" />
    <platform name="browser">
        <hook type="before_prepare" src="hooks/appBrowserCopyFromPlatformToWWW.sh" />
    </platform>
    <platform name="ios">
